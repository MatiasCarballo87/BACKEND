<h1>Vista de Productos</h1>

<div id="allproducts"></div>

<h2>Ingrese un Producto</h2>

<div class="container">
    <form id="formCreate" class="formulario">
        <label>Name</label>
        <input type="text"  name="title">
        <label>Description</label>
        <input type="text" name="description">
        <label>Price</label>
        <input type="number" name="price" >
        <label>URL</label>
        <input type="text" name="thumbnail" >
        <label>Code</label>
        <input type="number" name="code" >
        <label>Stock</label>
        <input type="number" name="stock" >
        <input type="submit" value="Send" class="button"/>
    </form>
</div>  

<div class="container-two"> 
    <form id="formDelete" class="formulario">
        <label>Product to Delete</label>
        <input type="number" name="id" placeholder="insert ID..." >
        <input type="submit" value="Send" class="button-two"/>
    </form>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="../public/index-socket.js"></script>